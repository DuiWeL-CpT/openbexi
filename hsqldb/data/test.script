CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE COMPANY(_ID_COMPANY VARCHAR(254) NOT NULL PRIMARY KEY,NAME VARCHAR(254),TICKER VARCHAR(254),MKTCAP VARCHAR(254),SALES VARCHAR(254),EMPLOYEES VARCHAR(254))
CREATE UNIQUE INDEX _INDEX_COMPANY ON COMPANY(_ID_COMPANY)
CREATE MEMORY TABLE PAINTING(_ID_PAINTING VARCHAR(254) NOT NULL PRIMARY KEY,NAME VARCHAR(254),DATE VARCHAR(254),ARTIST VARCHAR(254),PICTURE VARCHAR(254),WIKIPEDIA VARCHAR(254),CHECKED VARCHAR(254))
CREATE UNIQUE INDEX _INDEX_PAINTING ON PAINTING(_ID_PAINTING)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
